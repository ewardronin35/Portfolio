{
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.7.3"
    }
  },
  "routes": [
    { "src": "/favicon.ico", "dest": "/public/icon.ico" },
    { "src": "/assets/(.*)", "dest": "/public/$1" },
    { "src": "/(.*\\.js)", "dest": "/public/$1" },
    { "src": "/(.*\\.css)", "dest": "/public/$1" },
    { "src": "/(.*\\.png|.*\\.jpg|.*\\.ico)", "dest": "/public/$1" },
    { "src": "/test", "dest": "/api/test.php" },
    { "src": "/(.*)", "dest": "/api/index.php" }
  ]
}